"def assess_signal_quality(signal):\n",
"    """评估PPG信号质量"""\n",
"    quality_metrics = {}\n",
"    \n",
"    # 1. 信噪比估计\n",
"    signal_power = np.var(signal)\n",
"    # 估计噪声（高频成分）\n",
"    diff_signal = np.diff(signal)\n",
"    noise_power = np.var(diff_signal)\n",
"    snr = 10 * np.log10(signal_power / noise_power) if noise_power > 0 else float('inf')\n",
"    quality_metrics['SNR (dB)'] = snr\n",
"    \n",
"    # 2. 信号稳定性（变异系数）\n",
"    cv = np.std(signal) / np.abs(np.mean(signal)) if np.mean(signal) != 0 else 0\n",
"    quality_metrics['变异系数'] = cv\n",
"    \n",
"    # 3. 峰值检测质量\n",
"    from scipy.signal import find_peaks\n",
"    peaks, _ = find_peaks(signal, distance=20, prominence=0.1)\n",
"    peak_regularity = np.std(np.diff(peaks)) if len(peaks) > 1 else float('inf')\n",
"    quality_metrics['峰值规律性'] = peak_regularity\n",
"    quality_metrics['检测到的峰值数'] = len(peaks)\n",
"    \n",
"    # 4. 整体质量评分\n",
"    quality_score = 0\n",
"    if snr > 20: quality_score += 25\n",
"    elif snr > 15: quality_score += 20\n",
"    elif snr > 10: quality_score += 15\n",
"    \n",
"    if cv < 0.5: quality_score += 25\n",
"    elif cv < 1.0: quality_score += 20\n",
"    elif cv < 1.5: quality_score += 15\n",
"    \n",
"    if peak_regularity < 10: quality_score += 25\n",
"    elif peak_regularity < 20: quality_score += 20\n",
"    elif peak_regularity < 30: quality_score += 15\n",
"    \n",
"    if 5 <= len(peaks) <= 15: quality_score += 25\n",
"    elif 3 <= len(peaks) <= 20: quality_score += 20\n",
"    \n",
"    quality_metrics['质量评分'] = quality_score\n",
"    \n",
"    return quality_metrics\n",
"\n",
"# 评估信号质量\n",
"quality_metrics = assess_signal_quality(processed_signal)\n",
"\n",
"print("PPG信号质量评估:")\n",
"print("-" * 30)\n",
"for metric, value in quality_metrics.items():\n",
"    if isinstance(value, float):\n",
"        print(f"{metric}: {value:.2f}")\n",
"    else:\n",
"        print(f"{metric}: {value}")\n",
"\n",
"# 质量评分可视化\n",
"quality_score = quality_metrics['质量评分']\n",
"if quality_score >= 80:\n",
"    quality_level = "优秀"\n",
"    color = "green"\n",
"elif quality_score >= 60:\n",
"    quality_level = "良好"\n",
"    color = "orange"\n",
"elif quality_score >= 40:\n",
"    quality_level = "一般"\n",
"    color = "yellow"\n",
"else:\n",
"    quality_level = "较差"\n",
"    color = "red"\n",
"\n",
"print(f"\n整体信号质量: {quality_level} ({quality_score}/100)")\n",
"\n",
"# 绘制质量评分仪表盘\n",
"fig, ax = plt.subplots(figsize=(8, 6))\n",
"theta = np.linspace(0, np.pi, 100)\n",
"r = np.ones_like(theta)\n",
"\n",
"ax = plt.subplot(111, projection='polar')\n",
"ax.plot(theta, r, 'k-', linewidth=3)\n",
"ax.fill_between(theta, 0, r, alpha=0.3)\n",
"\n",
"# 添加质量指针\n",
"pointer_angle = np.pi * (1 - quality_score / 100)\n",
"ax.plot([pointer_angle, pointer_angle], [0, 0.8], color=color, linewidth=4)\n",
"ax.plot(pointer_angle, 0.8, 'o', color=color, markersize=10)\n",
"\n",
"ax.set_ylim(0, 1)\n",
"ax.set_theta_zero_location('W')\n",
"ax.set_theta_direction(1)\n",
"ax.set_title(f'信号质量评分: {quality_score}/100 ({quality_level})', \n",
"             fontsize=14, fontweight='bold', pad=20)\n",
"\n",
"# 添加刻度标签\n",
"ax.set_thetagrids([0, 45, 90, 135, 180], ['100', '75', '50', '25', '0'])\n",
"ax.set_rgrids([])\n",
"\n",
"plt.show()"
]
