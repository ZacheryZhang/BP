   "source": [
    "def generate_synthetic_ppg(length=1000, sampling_rate=125, heart_rate=70):\n",
    "    \"\"\"\n",
    "    生成合成PPG信号\n",
    "    \n",
    "    Args:\n",
    "        length: 信号长度\n",
    "        sampling_rate: 采样率\n",
    "        heart_rate: 心率 (bpm)\n",
    "    \n",
    "    Returns:\n",
    "        合成的PPG信号\n",
    "    \"\"\"\n",
    "    t = np.linspace(0, length/sampling_rate, length)\n",
    "    \n",
    "    # 基础心跳频率\n",
    "    f_heart = heart_rate / 60  # Hz\n",
    "    \n",
    "    # 主要PPG波形（基于心跳）\n",
    "    ppg_main = np.sin(2 * np.pi * f_heart * t)\n",
    "    \n",
    "    # 添加谐波分量\n",
    "    ppg_harmonic2 = 0.3 * np.sin(2 * np.pi * 2 * f_heart * t + np.pi/4)\n",
    "    ppg_harmonic3 = 0.1 * np.sin(2 * np.pi * 3 * f_heart * t + np.pi/3)\n",
    "    \n",
    "    # 呼吸波形\n",
    "    f_resp = 0.25  # 呼吸频率 15次/分钟\n",
    "    ppg_resp = 0.2 * np.sin(2 * np.pi * f_resp * t)\n",
    "    \n",
    "    # 添加噪声\n",
    "    noise = 0.05 * np.random.normal(0, 1, length)\n",
    "    \n",
    "    # 组合信号\n",
    "    ppg_signal = ppg_main + ppg_harmonic2 + ppg_harmonic3 + ppg_resp + noise\n",
    "    \n",
    "    # 添加基线漂移\n",
    "    baseline = 0.1 * np.sin(2 * np.pi * 0.05 * t)\n",
    "    ppg_signal += baseline\n",
    "    \n",
    "    return ppg_signal\n",
    "\n",
    "# 生成示例PPG信号\n",
    "ppg_signal = generate_synthetic_ppg(length=1000, heart_rate=75)\n",
    "\n",
    "print(f\"生成PPG信号: 长度={len(ppg_signal)}, 均值={np.mean(ppg_signal):.3f}, 标准差={np.std(ppg_signal):.3f}\")"
   ]
